{
  "name": "@socialwell-design/tool-tip",
  "version": "1.0.1",
  "description": "ToolTip component from socialwell ui library",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
